---
type IconProps = {
    name: string;
};

const { name } = Astro.props as IconProps;
---

<style>
    .icon-element {
        display: inline-block;
        vertical-align: middle;
        fill: currentColor;
    }
</style>

<img
    src=`/src/icons/src/chunk/${name}.svg`
    width="12"
    height="12"
    class="icon-element icon-primary"
/>
