---
import { Image } from "astro:assets";

interface Props {
  className?: string;
}

const { className = "pointer-events-none absolute rotate-12 top-[196px] right-[-40px] lg:right-[-140px] z-[1] opacity-10 lg:opacity-100" } = Astro.props;
---

<Image
    width="330"
    height="158"
    class={className}
    src="/feris_sticker.png"
    alt=""
/>