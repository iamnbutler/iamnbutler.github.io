---
import HostedImage from "../HostedImage.astro";

interface Props {
    className?: string;
}

const {
    className = "top-[-96px] left-[40px] z-[1] lg:top-[-120px] lg:left-[60px] pointer-events-none absolute rotate-12 opacity-10 lg:opacity-100",
} = Astro.props;
---

<HostedImage
    width="192"
    height="192"
    class={className}
    src="stickers/old_zed_sticker.png"
    alt=""
/>
